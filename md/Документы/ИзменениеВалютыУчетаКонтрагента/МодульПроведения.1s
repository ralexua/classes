Процедура ОбработкаПроведения()   
	сч = 0;
	ВыбратьСтроки();
	Пока ПолучитьСтроку() = 1 Цикл   
		состояние("Строка "+сч);
		Если постПок = Перечисление.поставщикПокупатель.Поставщик Тогда
			Регистр.ВзаиморасчетыПоставщиков.ПривязыватьСтроку(НомерСтроки);	 
			Регистр.ВзаиморасчетыПоставщиков.ДвижениеПриход(Фирма1,выбКонтрагент,Договор,СтавкаНДС,КредДокумент,-1*Долг,,,);
			Регистр.ВзаиморасчетыПоставщиков.ДвижениеПриход(Фирма1,новКонтрагент,Договор,СтавкаНДС,ТекущийДокумент(),новДолг,,,);
			сч  = сч+1;
		Иначе
			Регистр.ВзаиморасчетыПокупателей.ПривязыватьСтроку(НомерСтроки);	 
			Регистр.ВзаиморасчетыПокупателей.ДвижениеПриход(Фирма1,выбКонтрагент,Договор,СтавкаНДС,КредДокумент,-1*Долг,,,);
			Регистр.ВзаиморасчетыПокупателей.ДвижениеПриход(Фирма1,новКонтрагент,Договор,СтавкаНДС,ТекущийДокумент(),новДолг,,,);
			сч  = сч+1;
		КонецЕсли;
	КонецЦикла;
